algoritmo "exresolvidoscreuza2"
var
   dinheiro, dolares, dolar: real
inicio
      Escreva("Quantos reais você tem? ")
      Leia(dinheiro)
      Escreva("Quanto está o dólar hoje? ")
      Leia(dolar)
      dolares <- dinheiro/dolar
      Escreva("Com ", dinheiro, " reais, você conseguirá comprar ", dolares," dólares.")
fimalgoritmo
